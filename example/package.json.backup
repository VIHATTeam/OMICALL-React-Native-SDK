{
  "name": "OmikitPluginExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "reset": "react-native start --reset-cache",
    "fix-android": "pkill -f metro || true && cd android && ./gradlew clean && cd .. && npx react-native start --reset-cache & sleep 3 && npx react-native run-android",
    "clean-all": "rm -rf node_modules && npm install && cd android && ./gradlew clean && cd .. && npx react-native start --reset-cache",
    "fix-metro": "pkill -f metro || true && rm -rf /tmp/metro-* && rm -rf /tmp/haste-* && npx react-native start --reset-cache --verbose",
    "fix-404": "pkill -f metro || true && rm -rf /tmp/metro-* && watchman watch-del-all && rm -rf node_modules && npm install && npx react-native start --reset-cache",
    "dev": "pkill -f metro || true && npx react-native start --reset-cache & sleep 4 && npx react-native run-android",
    "sync-native": "cd .. && ./sync_native.sh",
    "sync-android": "cd .. && ./sync_native.sh android",
    "sync-ios": "cd .. && ./sync_native.sh ios",
    "watch-native": "cd .. && ./watch_native.sh"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.1",
    "@react-native-firebase/app": "^21.6.1",
    "@react-native-firebase/messaging": "^21.6.1",
    "@react-native/gradle-plugin": "^0.76.6",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "react": "18.3.1",
    "react-native": "0.76.2",
    "react-native-device-info": "^11.1.0",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-modal": "^13.0.1",
    "react-native-permissions": "5.4.1",
    "react-native-reanimated": "3.17.4",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^4.5.0",
    "omikit-plugin": "file:.."
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.6",
    "@react-native/eslint-config": "0.76.6",
    "@react-native/metro-config": "0.76.6",
    "@react-native/typescript-config": "0.76.6",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
